{
  "name": "staxly",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "author": "Philip Schatz <phil@cnx.org>",
  "license": "ISC",
  "repository": "https://github.com/openstax/staxly.git",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "lint": "standard --fix",
    "start": "probot run ./index.cjs",
    "test": "IGNORE_FOR_TESTING=true jest && standard --fix",
    "test:debug": "NODE_ENV=development IGNORE_FOR_TESTING=true node --inspect-brk $(npm bin)/jest -i --updateSnapshot",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@slack/client": "^4.0.0",
    "probot": "^12.0.0",
    "probot-addon-settings": "github:probot/settings#af40e21548181e8221a01bbcf125d1ee28f50a48",
    "probot-config": "^1.1.0",
    "sax": "^1.2.4",
    "smee-client": "^1.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "jest": "^26.1.0",
    "nock": "^12.0.0",
    "nodemon": "^1.14.11",
    "standard": "^16"
  },
  "engines": {
    "node": ">= 12.13.1",
    "npm": ">= 6.11.3"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}